var $currentDiv = "div#" + "<%= (@answer.id) %>";
var currentElementVoteCount = parseInt(<%= @answer.vote %>);
var nextElementVoteCount = parseInt($($currentDiv).next().attr("data-vote-count"));
var previousElementVoteCount = parseInt($($currentDiv).prev().attr("data-vote-count"));

$("#" + "<%= (@answer.id) %>" + " .vote").text("Vote: " + "<%= (@answer.vote) %>");
$($currentDiv).attr("data-vote-count", "<%= @answer.vote %>")
if (currentElementVoteCount < nextElementVoteCount) {
  $($currentDiv).insertAfter($($currentDiv).next());
}

