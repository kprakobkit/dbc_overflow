var $currentDiv = "div#" + "<%= (@question.id) %>";
var currentElementVoteCount = parseInt(<%= @question.vote %>);
var nextElementVoteCount = parseInt($($currentDiv).next().attr("data-vote-count"));
var previousElementVoteCount = parseInt($($currentDiv).prev().attr("data-vote-count"));

$("#" + "<%= (@question.id) %>" + " .vote").text("Vote: " + "<%= (@question.vote) %>");
$($currentDiv).attr("data-vote-count", "<%= @question.vote %>");
if (currentElementVoteCount > previousElementVoteCount) {
  $($currentDiv).insertBefore($($currentDiv).prev());
}
