var $currentDiv = "div#" + "<%= (@question.id) %>";
var currentElementVoteCount = parseInt(<%= @question.vote %>, 10);
var nextElementVoteCount = function(){ return parseInt($($currentDiv).next().attr("data-vote-count"))};
var previousElementVoteCount = function(){ return parseInt($($currentDiv).prev().attr("data-vote-count"))};

$("#" + "<%= (@question.id) %>" + " .vote").text("Vote: " + "<%= (@question.vote) %>")
$($currentDiv).attr("data-vote-count", "<%= @question.vote %>")
while (currentElementVoteCount < nextElementVoteCount()) {
  $($currentDiv).insertAfter($($currentDiv).next());
}
