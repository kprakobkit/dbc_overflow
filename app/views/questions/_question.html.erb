<div id="<%= question.id %>" data-vote-count="<%= question.vote %>">
<p>Question: <%= question.title %></p>
<p class="vote">Vote: <%= question.vote %></p>
<%= link_to 'Upvote', upvote_question_path(question), remote: true, method: :patch %>
<%= link_to 'Downvote', downvote_question_path(question), remote: true, method: :patch %>
<%= link_to 'Show', question_path(question) %>
<%= link_to 'Edit', edit_question_path(question) %>
<hr />
</div>
